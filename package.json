  "scripts": {
    "create-tables": "yarn run sequelize db:migrate",
    "insert-data": "yarn run sequelize db:seed:all",
    "delete-data": "yarn run sequelize db:seed:undo:all",
    "drop-tables": "yarn run sequelize db:migrate:undo:all",
    "reset-data": "yarn run delete-data && yarn run drop-tables && yarn run create-tables && yarn run insert-data",
    "watch": "./node_modules/.bin/nodemon",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
